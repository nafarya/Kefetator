func condtest(a) {
  if (1) {
    a = 5;
    if (0) {
      a = 7;
    } else {
      a = 6;
    }
  }
  return a;
}

func f(n) {
  if (n) {
    return n * f( (n - 1) );
  } else {
    return 1;
  }
}

func testCond() {
  c = condtest(2);
  print(c);
  return 0;
}

func main() {
  n = readInt();
  for i = n; i; i = i - 1 {
    print( f(i) );
  }
  return 0;
}